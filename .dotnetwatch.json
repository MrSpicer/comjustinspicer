{
  "watch": [
    "Comjustinspicer.Web/",
    "Comjustinspicer.CMS/",
    "Comjustinspicer.Tests/"
  ],
  "ignore": [
    "**/bin/**",
    "**/obj/**",
    "**/node_modules/**",
    "**/.git/**",
    "**/.vs/**",
    "**/.vscode/**",
    "**/Logs/**",
    "**/.dotnet/**",
    "**/TestResults/**",
    "**/.idea/**",
    "**/*.scss"
  ],
  "reload": {
    "include": [
      "**/*.cs",
      "**/*.cshtml",
      "**/*.csproj",
      "**/*.json",
      "**/*.css",
      "**/*.js"
    ]
  }
}
